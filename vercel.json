{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "dist/gorest-users-fe/browser",
  "github": {
    "silent": true
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "build": {
    "env": {
      "NG_APP_API_TOKEN": "@api_token",
      "NG_APP_API_URL": "@api_url"
    }
  }
}
